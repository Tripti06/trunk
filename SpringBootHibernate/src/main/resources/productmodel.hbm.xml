<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 5.3//EN" "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">

<hibernate-mapping>
	<class name="com.example.springboothibernate.model.ProductsModel"
		table="Products_Table">
		<id name="productId">
			<generator class="assigned"></generator>
		</id>

		<property name="productName"></property>
		<property name="productAmount"></property>
	</class>
</hibernate-mapping>  